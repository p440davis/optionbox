{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "ENV": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "double"],
        "semi": ["error", "always"]
    }
}
